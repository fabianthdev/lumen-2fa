{
    "name": "lshtmweb/lumen-2fa",
    "description": "A One Time Password Authentication package, compatible with Google Authenticator. Authenticates oauth tokens",
    "keywords": ["authentication", "two factor authentication", "google2fa", "laravel", "lumen"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "prog_24",
            "email": "promise.ekoriko@lshtm.ac.uk",
            "role": "Technical Lead"
        }
    ],
    "require": {
        "php": ">=7.0",
        "pragmarx/google2fa-laravel": "~2"
    },
    "require-dev": {
        "phpunit/phpunit": "~5|~6|~7|~8|~9",
        "orchestra/testbench-browser-kit": "3.4.*|3.5.*|3.6.*|3.7.*|4.*|5.*|6.*"
    },
    "autoload": {
        "psr-4": {
            "Lshtmweb\\Lumen2FA\\": "src/",
            "Lshtmweb\\Lumen2FA\\Tests\\": "tests/"
        }
    },
    "extra": {
        "component": "package",
        "frameworks": ["Lumen"]
    },
    "suggest": {
      "bacon/bacon-qr-code": "Required to generate inline QR Codes.",
      "pragmarx/recovery": "Generate recovery codes.",
      "pragmarx/google2FA": "For use with full Laravel packages."
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": false
        }
    }
}
